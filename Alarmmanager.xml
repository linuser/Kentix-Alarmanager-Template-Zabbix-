                            <snmp_oid>.1.3.6.1.4.1.37954.5.2.3.1.2.{#SENSORINDEX}</snmp_oid>
                            <key>kentixdevices.humValue[{#SENSORINDEX}]</key>
                            <value_type>FLOAT</value_type>
                            <units>%</units>
                            <preprocessing>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <parameters>
                                        <parameter>0.1</parameter>
                                    </parameters>
                                </step>
                            </preprocessing>
                            <tags>
                                <tag>
                                    <tag>Application</tag>
                                    <value>Kentix Sensors</value>
                                </tag>
                            </tags>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <uuid>96c0018e57ab4705a7a4233ed776fc15</uuid>
