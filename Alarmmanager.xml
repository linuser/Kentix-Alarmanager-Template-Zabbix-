                    <delay>1h</delay>
                    <lifetime>0</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <uuid>9fe39ee0dfe04e83b3e196231901a553</uuid>
                            <name>{#SENSORNAME} Taupunkt</name>
