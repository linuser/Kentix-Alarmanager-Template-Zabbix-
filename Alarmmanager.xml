            <discovery_rules>
                <discovery_rule>
                    <uuid>07a0916c51af4300858d9a759570fb73</uuid>
                    <name>Sensor Discovery</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>discovery[{#SENSORINDEX}, .1.3.6.1.4.1.37954.5.2.1.1.1, {#SENSORNAME}, .1.3.6.1.4.1.37954.5.2.1.1.2]</snmp_oid>
